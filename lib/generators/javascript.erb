const elements = [
  <% groups.each.with_index do |group, index| %>
    <%= ", " unless index.zero? %><%= JSON.generate({:data => {:id => group}}) %>
  <% end %>

  <% nodes.each.with_index do |node, index| %>
    <%= ", " unless index.negative? %><%= JSON.generate({:data => {:id => node.data.key, :parent => node.data.level}}) %>
  <% end %>

  <% edges.each.with_index do |edge, index| %>
    , <%= JSON.generate({:data => {:id => "edge#{index}", :source => edge.source.data.key, :target => edge.target.data.key}}) %>
  <% end %>
];

export default elements;
